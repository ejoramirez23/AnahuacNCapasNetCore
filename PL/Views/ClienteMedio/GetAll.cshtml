@{
    ViewData["Title"] = "GetAll";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
 <link rel="stylesheet" href="~/css/ClienteMedio/MedioCards.css"/> 

<div class="container mt-5">
    <div class="row">
        <div class="col-md-3">
            <div class="card" id="cardMedio">
               
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">

    $(document).ready(function () {
        GetAll();
    });


    function GetAll(){
        $.ajax({
            type: 'GET',
            url: '@Url.Action("MedioGetAll")',
            success: function (result){
               
                $.each(result.objects, function (i, medio) {


                    var content = '<div class="image-container" >'
                        + '<img src=" data:image/png;base64,' + medio.imagen + '" class="img-fluid rounded thumbnail-image">'
                        + '<a class="btn btn-info" @Url.Action("Form", "Solicitud")> Solicitar</a>'
                        + '</div>
                    $("#cardMedio").append(content);

                });
            },
        });
    };
</script>

